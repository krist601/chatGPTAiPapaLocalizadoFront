{
    "/api": {

       "target": "http://localhost:3000",
        "secure": false,
        "changeOrigin": true,
        "pathRewrite": {
            "^/api": ""
        },
        "headers": {
            "ngrok-skip-browser-warning": "true",
            "User-Agent": "Angular-Local-Proxy",
            "Origin": "http://localhost:3000"
        },
        "logLevel": "debug"
    },
    "/s3-proxy": {
        "target": "https://360images-fhd-street.s3.us-east-2.amazonaws.com",
        "secure": false,
        "changeOrigin": true,
        "pathRewrite": {
            "^/s3-proxy": ""
        },
        "logLevel": "debug"
    }
}